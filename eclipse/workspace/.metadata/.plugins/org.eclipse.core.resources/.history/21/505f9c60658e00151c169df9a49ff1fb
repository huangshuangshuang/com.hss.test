package com.haowu.interest;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;

public class Test {
	private static StringBuilder sql = new StringBuilder(
			"insert into user (name,idCard,sex,age,number,address) values(");
	private static File file;
	private static FileInputStream reader;
	private static FileOutputStream outputStream;
	static{
		try {
			reader=new FileInputStream(file);
			outputStream=new FileOutputStream(file);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}
	public static void main(String[] args) {
		byte[] buff=new byte[1024];
		int len=0;
		for (int i = 0; i < 10000000; i++) {
			User user = User.build();
			sql.append(user.getName() + ",");
			sql.append(user.getIdCard() + ",");
			sql.append(user.getSex() + ",");
			sql.append(user.getAge() + ",");
			sql.append(user.getNumber() + ",");
			sql.append(user.getAddress() + ");+\r\n");
		}
		
		try {
			while ((len=reader.read(buff))!=-1) {
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
		System.out.println("执行完毕");
	}
}
