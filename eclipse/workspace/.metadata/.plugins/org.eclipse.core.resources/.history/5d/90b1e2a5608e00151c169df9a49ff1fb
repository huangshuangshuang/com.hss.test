package com.haowu.interest;

public class Test {
	private static String sql="insert into user (name,idCard,sex,age,number,address) values(?,?,?,?,?,?)";
	private static JDBCUtil util;
    public static void main(String[] args) {
    	util=new JDBCUtil();
        for (int i = 0; i < 1000000; i++) {
        	User user=User.getUser();
        	try {
				util.update(sql, user);
			} catch (Exception e) {
				e.printStackTrace();
			}
		}
        System.out.println("执行完毕");
    }
}
