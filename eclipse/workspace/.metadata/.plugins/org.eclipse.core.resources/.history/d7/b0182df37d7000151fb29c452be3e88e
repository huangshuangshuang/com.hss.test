package com.hss.game.util;

import org.jaudiotagger.audio.mp3.MP3File;

public class Util {
	private static String filePath = "image/music1.mp3";
	//private static final String fileUrl = "file://C:/Users/300002/Desktop/music1.mp3";
	public static void main(String[] args) {
		try {
			MP3File mp3File=new MP3File(filePath);
			System.err.println(mp3File.getAudioHeader());;
			mp3File.commit();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
}
